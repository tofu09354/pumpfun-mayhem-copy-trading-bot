# Server Configuration
PORT=3002
NODE_ENV=development

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=your_wallet_private_key_here
SOLANA_PUBLIC_KEY=your_wallet_public_key_here

# Copy Trading Configuration
COPY_ENABLED=true
TARGET_WALLETS=wallet1,wallet2,wallet3
POSITION_SIZE_MODE=percentage
POSITION_SIZE_VALUE=10
MAX_POSITION_SIZE=0.5
EXECUTION_DELAY_MS=100

# Risk Management
MAX_DAILY_LOSS=2.0
MAX_OPEN_POSITIONS=10
BLACKLIST_TOKENS=
WHITELIST_TOKENS=

# Filters
MIN_TRADE_SIZE=0.01
MAX_TRADE_SIZE=10.0
FILTER_BY_TOKEN=true
FILTER_BY_SIZE=true

# Logging
LOG_LEVEL=info
LOG_FILE=logs/copy-trading.log

